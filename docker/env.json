{
  "host_env_var_prefix": "DIRECTORY_FORMS_API",
  "file_path": "./docker/.env",
  "env_vars": {
    "required": [
      "PORT",
      "SECRET_KEY",
      "DATABASE_URL",
      "HEALTH_CHECK_TOKEN",
      "ZENDESK_SUBDOMAIN",
      "ZENDESK_TOKEN",
      "ZENDESK_EMAIL",
      "EMAIL_HOST",
      "EMAIL_HOST_PASSWORD",
      "EMAIL_HOST_USER",
      "EMAIL_PORT",
      "EMAIL_USE_TLS",
      "DEFAULT_FROM_EMAIL",
      "GOV_NOTIFY_API_KEY",
      "ZENDESK_SUBDOMAIN_EUEXIT",
      "ZENDESK_TOKEN_EUEXIT",
      "ZENDESK_EMAIL_EUEXIT",
      "ZENDESK_CUSTOM_FIELD_ID",
      "ZENDESK_CUSTOM_FIELD_ID_EUEXIT"
    ],
    "optional": [
      "DEBUG",
      "REMOTE_IP_ADDRESS_RETRIEVER",
      "CSRF_COOKIE_SECURE",
      "SESSION_COOKIE_SECURE",
      "SESSION_COOKIE_DOMAIN",
      "EMAIL_BACKEND_CLASS_NAME"
    ]
  }
}
